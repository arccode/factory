{
  "backends": {
    "time_sanitizer": {
      "args": {
        "base_time_files": ["/usr/local/etc/lsb-factory",
                            "/etc/lsb-release"]
      }
    },
    "system_log_manager": {
      "args": {
        "scan_log_period_secs": 120,
        "sync_log_period_secs": 600,
        "sync_log_paths": [
          "/var/factory/log/",
          "/var/log/messages",
          "/var/log/Xorg.0.log",
          "/var/log/bios_info.txt",
          "/var/log/ec_info.txt"],
        "clear_log_paths": [
          "/var/log/connectivity.log",
          "/var/log/connectivity.bak",
          "/var/log/net.log",
          "/var/log/net.*.log",
          "/var/log/messages*"],
        "clear_log_excluded_paths": [
          "/var/log/messages", "/var/log/messages.[12]"],
        "enable_foreground_sync": true
      }
    },
    "instalog": {
      "args": {
        "uplink_hostname": "localhost",
        "uplink_port": 7500,
        "uplink_use_shopfloor": true
      }
    }
  }
}
