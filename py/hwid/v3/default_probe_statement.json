{
  "audio_codec": {
    "generic": {
      "eval": "generic_audio_codec"
    }
  },
  "battery": {
    "generic": {
      "eval": "generic_battery",
      "keys": [
        "manufacturer",
        "model_name",
        "technology",
        "type"
      ]
    }
  },
  "bluetooth": {
    "generic": {
      "eval": "generic_bluetooth",
      "keys": [
        "device",
        "idProduct",
        "idVendor",
        "bcdDevice",
        "vendor",
        "revision_id"
      ]
    }
  },
  "mainboard": {
    "generic": {
      "eval": {
        "shell": {
          "command": "mosys platform version",
          "key": "version"
        }
      }
    }
  },
  "cellular": {
    "generic": {
      "eval": "generic_network_device:cellular",
      "keys": [
        "device",
        "idProduct",
        "idVendor",
        "bcdDevice",
        "vendor",
        "revision_id"
      ]
    }
  },
  "chassis": {
    "generic": {
      "eval": {
        "shell": {
          "command": "mosys platform chassis",
          "key": "id"
        }
      }
    }
  },
  "cpu": {
    "generic": {
      "eval": "generic_cpu",
      "keys": [
        "model",
        "cores",
        "hardware"
      ]
    }
  },
  "display_panel": {
    "generic": {
      "eval": "edid",
      "keys": [
        "vendor",
        "product_id",
        "width",
        "height"
      ]
    }
  },
  "dram": {
    "generic": {
      "eval": "generic_dram"
    }
  },
  "ec_flash_chip": {
    "generic": {
      "eval": "flash_chip:ec"
    }
  },
  "embedded_controller": {
    "generic": {
      "eval": [
        {
          "shell": {
            "command": "mosys ec info -s vendor",
            "key": "vendor"
          }
        },
        {
          "shell": {
            "command": "mosys ec info -s name",
            "key": "name"
          }
        }
      ]
    }
  },
  "ethernet": {
    "generic": {
      "eval": "generic_network_device:ethernet",
      "keys": [
        "product",
        "vendor",
        "revision_id"
      ]
    }
  },
  "flash_chip": {
    "generic": {
      "eval": "flash_chip:main"
    }
  },
  "pmic": {
    "generic": {
      "eval": {
        "glob_path": {
          "pathname": "/sys/bus/platform/devices/*-pmic",
          "filename_only": true
        }
      }
    }
  },
  "storage": {
    "generic": {
      "eval": "generic_storage",
      "keys":[
        "vendor",
        "model",
        "type",
        "name",
        "hwrev",
        "oemid",
        "manfid",
        "prv",
        "device",
        "class",
        "sectors"
      ]
    }
  },
  "stylus": {
    "generic": {
      "eval": {
        "input_device": {
          "device_type": "stylus"
        }
      },
      "expect": {
        "product": "!re (?!0000)....$",
        "vendor": "!re (?!0000)....$"
      },
      "keys": [
        "name",
        "product",
        "vendor"
      ]
    }
  },
  "touchpad": {
    "generic": {
      "eval": {
        "input_device": {
          "device_type": "touchpad"
        }
      },
      "expect": {
        "product": "!re (?!0000)....$",
        "vendor": "!re (?!0000)....$"
      },
      "keys": [
        "name",
        "product",
        "vendor"
      ]
    }
  },
  "touchscreen": {
    "generic": {
      "eval": {
        "concat": {
          "functions": [
            {
              "input_device": {
                "device_type": "touchscreen"
              }
            },
            "touchscreen_i2c"
          ]
        }
      },
      "expect": {
        "vendor": "!re (?!0000)....$"
      },
      "keys": [
        "name",
        "product",
        "vendor",
        "fw_version"
      ]
    }
  },
  "tpm": {
    "generic": {
      "eval": "generic_tpm"
    }
  },
  "usb_hosts": {
    "generic": {
      "eval": "generic_usb_hosts",
      "keys": [
        "device",
        "idProduct",
        "idVendor",
        "bcdDevice",
        "vendor",
        "revision_id"
      ]
    }
  },
  "video": {
    "generic": {
      "eval": {
        "concat": {
          "functions": [
            "generic_video",
            "camera_cros"
          ]
        }
      },
      "keys": [
        "bcdDevice",
        "bus_type",
        "device",
        "idProduct",
        "idVendor",
        "iden",
        "manufacturer",
        "name",
        "product",
        "revision_id",
        "type",
        "vendor"
      ]
    }
  },
  "wireless": {
    "generic": {
      "eval": "generic_network_device:wireless",
      "keys": [
        "device",
        "idProduct",
        "idVendor",
        "bcdDevice",
        "vendor",
        "revision_id"
      ]
    }
  },
  "firmware_keys": {
    "generic": {
      "eval": "chromeos_firmware:firmware_keys"
    }
  },
  "region": {
    "generic": {
      "eval": {
        "vpd": {
          "fields": ["region"],
          "key": "region_code"
        }
      }
    }
  },
  "ro_main_firmware": {
    "generic": {
      "eval": "chromeos_firmware:ro_main_firmware"
    }
  },
  "ro_ec_firmware": {
    "generic": {
      "eval": "chromeos_firmware:ro_ec_firmware"
    }
  },
  "ro_pd_firmware": {
    "generic": {
      "eval": "chromeos_firmware:ro_pd_firmware"
    }
  }
}
